
@for (int i = 0; i < 12; i++)
{
    if (i < myhealth)
    {
        <healthbubblealivep1>o</healthbubblealivep1>
    }
    else
    {
        <healthbubledead>o</healthbubledead>
    }
}

@code {

    [Parameter]
    public int myhealth { get; set; }
    public void HealthMinus()
    {
        GameScript.play1hp--;
        StateHasChanged();
    }
    public void HealthPlus()
    {
        GameScript.play1hp++;
        StateHasChanged();
    }
}
