
@for (int i = 0; i < 12; i++)
{
    if (i < myhealth)
    {
        <healthbubblealivep4>o</healthbubblealivep4>
    }
    else
    {
        <healthbubledead>o</healthbubledead>
    }
}

@code {

    [Parameter]
    public int myhealth { get; set; }
    public void HealthMinus()
    {
        GameScript.play4hp--;
        StateHasChanged();
    }
    public void HealthPlus()
    {
        GameScript.play4hp++;
        StateHasChanged();
    }
}
