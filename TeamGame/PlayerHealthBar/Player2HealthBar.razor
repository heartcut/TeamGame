
@for (int i = 0; i < 12; i++)
{
    if (i < myhealth)
    {
        <healthbubblealivep2>o</healthbubblealivep2>
    }
    else
    {
        <healthbubledead>o</healthbubledead>
    }
}

@code {

    [Parameter]
    public int myhealth { get; set; }
    public void HealthMinus()
    {
        GameScript.play2hp--;
        StateHasChanged();
    }
    public void HealthPlus()
    {
        GameScript.play2hp++;
        StateHasChanged();
    }
}
